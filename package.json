{
  "name": "softd",
  "type": "module",
  "scripts": {
    "check": "biome check src/*.ts && tsc --noEmit",
    "fix": "biome check --write --unsafe src/*.ts",
    "test": "vitest src/**/*.spec.ts",
    "build": "tsc -p .",
    "serve": "http-server ."
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "http-server": "^14.1.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  }
}
